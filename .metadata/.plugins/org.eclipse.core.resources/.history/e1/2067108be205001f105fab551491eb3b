@SuppressWarnings("serial")
public class Deconnexion extends AbstractAction 
	{
				
		/*public Deconnexion(Pageherit page, String texte)
		{
			super(texte);
     	}*/

		public void actionPerformed(ActionEvent e)
		{ 
			Connexion.fenetreprincipale.setVisible(false);
