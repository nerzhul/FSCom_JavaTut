package windows.forms;

import java.awt.Dimension;

import javax.swing.JFrame;

public class onglets extends JFrame{

	private static final long serialVersionUID = 1L;
	private JFrame fram;
	private String contact;
	
	public onglets(Object interlocuteur) {
		
		contact = interlocuteur.toString();
		
		fram = new JFrame();
		fram.setSize(new Dimension(200,200));
		fram.setTitle("Cookie Messenger - Conversation"); 
		fram.setSize(600,500); //420
		fram.setLocationRelativeTo(null);
		fram.setResizable(true);
		fram.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE); 		
			
		new PanelOnglet(contact,fram);	

		}
}
