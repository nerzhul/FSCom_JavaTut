package windows.actions;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JFrame;
import javax.swing.JOptionPane;

import user.*;

public class Clicks_Contact_Supprimer implements ActionListener {

	private Object contact;
	private JFrame fenetre;
	public Clicks_Contact_Supprimer(Object contactasupprimer) {
		this.contact=contactasupprimer;
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		//supprimercontact(contact);
		
		JOptionPane.showMessageDialog(fenetre,"Le contact "+ contact +" a été supprimé avec succès !");
	}
}
