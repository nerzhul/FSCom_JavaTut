package msn.pageconnec;

import java.awt.event.ActionEvent;

import javax.swing.AbstractAction;

import msn.menu.Connexion;
import msn.pageacceuil.Pageacceuil;
import msn.pageheritage.Page;



public class Deconnexion extends AbstractAction 
	{
	
		private static final long serialVersionUID = 3L;
		
				
		public Deconnexion(Page page, String texte)
		{
			super(texte);
     	}

		public void actionPerformed(ActionEvent e)
		{ 
			Connexion.fenetreconnectée.setVisible(false);
			Pageacceuil fenetre = new Pageacceuil();
			fenetre.setVisible(true);
		}
	}