package windows.actions;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JPanel;

import windows.window.form_connect;
import windows.window.form_option;

public class Menu_pref implements ActionListener {

	private JPanel pantemp;
	public Menu_pref(JPanel pan) {
		this.pantemp=pan;
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		new form_option(pantemp);

	}

}
