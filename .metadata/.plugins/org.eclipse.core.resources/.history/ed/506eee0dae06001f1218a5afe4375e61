package windows;

import javax.swing.JPanel;

public class masterwindow {

	private JPanel panel;
	public masterwindow(JPanel pan){
		panel = pan;
	}
	public void Hide(){
		panel.setVisible(false);
	}
	
	public void Show(){
		panel.setVisible(true);
	}
	
	public JPanel getPanel(){ return this.panel; }

}

